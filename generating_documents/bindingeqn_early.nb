   3.9281967237291875`*^9, {3.928196770744295*^9, 3.928196771497292*^9}, {
   3.9290475466424274`*^9, 3.929047725827614*^9}, {3.929048514249814*^9, 
   3.929048568680187*^9}, {3.929048608638398*^9, 3.9290487063735456`*^9}, {
